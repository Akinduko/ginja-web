{
  "compilerOptions": {
    "target": "es2018",
    "module": "commonjs",
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "+components": ["src/components"],
      "+store": ["src/store"],
      "+redux": ["src/redux"],
      "+containers": ["src/containers"],
      "+assets": ["src/assets"],
      "+styles": ["src/styles"],
      "+utils": ["src/utils"],
      "+services": ["src/services"],
      "+pages": ["src/pages"]
    }
  },
  "exclude": ["node_modules"]
}
